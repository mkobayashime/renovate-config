{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "rangeStrategy": "pin",
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchUpdateTypes": ["patch"],
      "groupName": "dependencies (patch)"
    },
    {
      "matchPackagePatterns": ["node"],
      "rangeStrategy": "replace"
    }
  ]
}
